{
  "name": "Material XKCD",
  "description": "Popup tab displaying XKCD comics with material design.",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Material XKCD",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "icons": {
      "128": "icon.png"
   },
  "manifest_version": 2
}
